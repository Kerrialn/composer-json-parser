{
    "name": "kerrialn/composer-json-parser",
    "description": "Access your composer.json as an object",
    "version": "1.1.0",
    "homepage" : "https://github.com/eventpoints/composer-json-parser",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "doctrine/orm": "^3.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KerrialNewham\\ComposerJsonParser\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KerrialNewham\\ComposerJsonParser\\Tests\\": "tests/"
        }
    },
    "replace": {
    },
    "scripts": {
        "rector": "vendor/bin/rector p --ansi",
        "rector-dry": "vendor/bin/rector p --dry-run --ansi",
        "phpstan": "php -d memory_limit=256M vendor/bin/phpstan analyze",
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "auto-scripts": {
        },
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "conflict": {
    },
    "extra": {
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.2",
        "symplify/easy-coding-standard": "^12"
    }
}
